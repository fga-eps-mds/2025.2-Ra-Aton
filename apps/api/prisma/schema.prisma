generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ProfileType {
  JOGADOR
  TORCEDOR
  ATLETICA
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  profileType ProfileType?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  passwordHash String
}
